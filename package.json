{
  "name": "imagekit-editor",
  "private": true,
  "description": "AI Image Editor powered by ImageKit",
  "keywords": [],
  "author": {
    "name": "ImageKit Developer",
    "email": "developer@imagekit.io",
    "url": "https://imagekit.io"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "start": "turbo run start",
    "build": "turbo run build",
    "version": "yarn workspace @imagekit/editor version",
    "package": "yarn build && yarn workspace @imagekit/editor pack --out ../../builds/%s-%v.tgz",
    "release": "yarn build && yarn workspace @imagekit/editor pack --out ../../builds/%s-%v.tgz && yarn workspace @imagekit/editor publish",
    "prepare": "husky",
    "lint": "biome ci"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.0",
    "husky": "^9.1.7",
    "turbo": "^2.0.1"
  },
  "packageManager": "yarn@4.9.2"
}
