{
  "name": "imagekit-editor",
  "private": true,
  "description": "AI Image Editor powered by ImageKit",
  "keywords": [],
  "author": {
    "name": "ImageKit Developer",
    "email": "developer@imagekit.io",
    "url": "https://imagekit.io"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "start": "turbo run start",
    "build": "turbo run build",
    "version": "yarn workspace @imagekit/editor version",
    "package": "yarn build && yarn workspace @imagekit/editor pack --out ../../builds/imagekit-editor-%v.tgz",
    "release": "yarn build && yarn workspace @imagekit/editor pack --out ../../builds/imagekit-editor-%v.tgz && yarn workspace @imagekit/editor publish",
    "prepare": "husky",
    "lint": "biome ci"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "turbo": "^2.0.1"
  },
  "packageManager": "yarn@4.9.2",
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
      "biome ci --files-ignore-unknown=true"
    ],
    "*": [
      "biome ci --files-ignore-unknown=true"
    ]
  },
  "dependencies": {
    "react-select": "^5.2.1"
  }
}
